<div id='modifyPassword' class='visible' style="display:none">
  <h3>Nouveau mot de passe</h3>
  <% if (locals.messages) { %>
 <p style="color:red"><%=  locals.messages.msg  %></p>
  <% } %>

    <form id="modifyPasswordForm">
      <fieldset>
        <div class="form-group">
          <label>Mot de passe</label>
          <input type="password" class="form-control" name="password" placeholder="Password" required="">
        </div>
        <div class="form-group">
          <label>Confirmer</label>
          <input type="password" class="form-control" name="passwordConf" placeholder="Confirm Password" required="">
        </div>
        <button type="submit" class="btn btn-primary" />ENREGISTRER</button>
      </fieldset>
    </form>


</div>
