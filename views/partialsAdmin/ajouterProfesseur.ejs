<form id="ajouterProfesseurForm" role="form">
  <fieldset>
    <div class="form-group has-feedback has-clear">
      <label for="prenom">Pr√©nom:</label>
      <input class="form-control" type="text" required name="prenom" />
    </div>
    <div class="form-group has-feedback has-clear">
      <label for="nom">Nom :</label>
      <input class="form-control" type="text" required name="nom" />
    </div>
    <div class="form-group has-feedback has-clear">
      <label for="grade">Grade :</label>
      <input class="form-control" type="text" name="grade" />
    </div>
    <div class="form-group">
      <button type="submit" class="btn btn-primary">Tester</button>
    </div>
  </fieldset>
</form>

<form id="validerProfesseurForm" role="form" style="display:none">
  <fieldset>
    <div class="form-group has-feedback has-clear">
      <label for="login">Login :</label>
      <input class="form-control" type="text" readonly required name="login" />
    </div>
    <div class="form-group has-feedback has-clear">
      <label for="password">Password:</label>
      <input class="form-control" type="text" readonly required name="password" />
    </div>
    <div class="form-group">
      <button type="reset" class="btn btn-secondary">Annuler</button>
      <button type="submit" class="btn btn-primary">Enregistrer</button>
    </div>
  </fieldset>
</form>
