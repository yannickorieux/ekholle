<div id="uploadEleves">
  <form id="uploadElevesForm" enctype="multipart/form-data">
    <fieldset>
      <!-- Default unchecked -->
      <div class="row">
        <div class="col-6">
          <div class="form-group has-feedback has-clear">
            <label>Fichier CSV</label>
            <!-- The name here is important, and will be used later to reference the data -->
            <input class='form-control' type="file" id="csvdata" name="csvdata" accept="text/csv">
          </div>
        </div>
        <div class="col-6">
          <div class="form-check">
            <input type="checkbox" class="form-check-input" id="importAnnuel">
            <label class="form-check-label" for="importAnnuel">Import annuel</label>
          </div>
        </div>
        <hr>
        
      </div>

      <div class="form-group">
        <button type="submit" class="btn btn-primary">Charger</button>
      </div>
    </fieldset>
  </form>
</div>
